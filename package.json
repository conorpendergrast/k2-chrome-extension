{
  "name": "k2-chrome-extension",
  "version": "0.0.3",
  "description": "A Chrome Extenstion for Kernel Schedule",
  "main": "index.js",
  "author": "Tim Golen <tim@golen.net>",
  "dependencies": {
    "alt": "^0.16.10",
    "browserify-shim": "^3.8.3",
    "browsernizr": "^1.0.2",
    "jquery": "^2.1.4",
    "moment": "^2.10.3",
    "react": "^0.13.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "crx": "^3.0.2",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "gulp": "^3.8.11",
    "gulp-copy": "0.0.2",
    "gulp-eslint": "^0.13.2",
    "gulp-ext-replace": "^0.2.0",
    "gulp-if": "^1.2.5",
    "gulp-import-css": "^0.1.2",
    "gulp-plumber": "^0.6.6",
    "gulp-react": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "minimist": "^1.1.1",
    "node-underscorify": "0.0.14",
    "run-sequence": "1.1.0",
    "through2": "^0.6.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "^2.5.0"
  },
  "browserify-shim": {
    "./public/lib/jquery/dist/jquery.js": {
      "exports": "$"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "node-underscorify",
        {
          "extensions": [
            "html"
          ],
          "requires": [
            {
              "variable": "_",
              "module": "underscore"
            }
          ]
        }
      ]
    ]
  }
}
